                  <Tooltip fx:id="tooltipNewFile" text="Empty Tooltip" />
               </tooltip>
            </ToggleSwitch>
            <ToggleSwitch fx:id="toggleSwitchRepairAll" text="Repair All" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
               <GridPane.margin>
                  <Insets bottom="5.0" />
               </GridPane.margin>
               <tooltip>
                  <Tooltip fx:id="tooltipRepairAll" text="Empty" />
               </tooltip>
            </ToggleSwitch>
         </children>
         <GridPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin>
      </GridPane>
      <Button fx:id="buttonOK" mnemonicParsing="false" onAction="#buttonOkContinue" text="OK" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
